xSeq <- seq(-2,3, by=0.1)
mean <- 0
sd <- 1
probability <- 0.975
quantile <- qnorm(probability,mean,sd)
cdfX <- pnorm(xSeq, mean = mean, sd = sd)
plot(xSeq, cdfX, type="l",xlab="x", ylab="F(x)")
abline(h=probability, col="red", lty=2)
